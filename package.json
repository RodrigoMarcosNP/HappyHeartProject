    "eslint-plugin-jest": "^28.8.3",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "globals": "^15.10.0",
    "hermes-eslint": "^0.24.0",
    "prettier": "^3.3.3",
    "typescript-eslint": "^8.8.1"
  },
  "dependencies": {
    "@react-navigation/elements": "^1.3.31"
  }
