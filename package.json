{
  "description": "Hosting User and Professional Apps for Old People Use",
  "type": "module",
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RodrigoMarcosNp/HappyHeartProject.git"
  },
  "engines": {
    "node": ">=20"
