{
  "name": "heart-user-server",
  "version": "0.0.0",
  "scripts": {
    "build": "tsc --build --clean && ts-node -r tsconfig-paths/register src/index.ts",
    "lint": "eslint src/**/*.ts",
    "start": "npm run lint && npm run build && npm run dev",
    "test": "npm run build && ava",
    "dev": "nodemon ./dist/src/index.js"
  },
  "dependencies": {
    "@types/pg": "^8.11.10",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "eslint-config-satya164": "^3.3.0",
    "express": "^4.21.1",
    "jsonwebtoken": "^9.0.2",
    "mysql": "^2.18.1",
    "pg": "^8.13.1",
    "start": "^5.1.0",
    "typeorm": "^0.2.37",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@ava/typescript": "^2.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^16.7.2",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "ava": "^3.15.0",
    "eslint": "^7.32.0",
    "fs-extra": "^10.0.0",
    "nodemon": "^3.1.7",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "~4.3"
  },
  "ava": {
    "files": [
      "test/*"
    ],
    "timeout": "1m",
    "typescript": {
      "rewritePaths": {
        "test/": "dist/test/"
      }
    }
  }
}
